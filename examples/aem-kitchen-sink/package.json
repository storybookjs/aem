{
  "name": "aem-kitchen-sink",
  "private": true,
  "version": "1.0.0",
  "description": "Fully featured aem storybook demo with events, knobs, docs, backgrounds and more",
  "scripts": {
    "build-storybook": "build-storybook",
    "sba": "sba",
    "storybook:debug": "NODE_OPTIONS=--inspect-brk start-storybook -p 9001",
    "storybook": "start-storybook -p 9001"
  },
  "license": "MIT",
  "dependencies": {
    "aem-sb-js-core-components": "*"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "6.0.0-alpha.2",
    "@storybook/addon-actions": "6.0.0-alpha.2",
    "@storybook/addon-backgrounds": "6.0.0-alpha.2",
    "@storybook/addon-centered": "6.0.0-alpha.2",
    "@storybook/addon-docs": "6.0.0-alpha.2",
    "@storybook/addon-events": "6.0.0-alpha.2",
    "@storybook/addon-jest": "6.0.0-alpha.2",
    "@storybook/addon-knobs": "6.0.0-alpha.2",
    "@storybook/addon-links": "6.0.0-alpha.2",
    "@storybook/addon-options": "6.0.0-alpha.2",
    "@storybook/addon-storyshots": "6.0.0-alpha.2",
    "@storybook/addon-storysource": "6.0.0-alpha.2",
    "@storybook/addon-viewport": "6.0.0-alpha.2",
    "@storybook/aem": "*",
    "chalk": "^3.0.0",
    "shelljs": "^0.8.3"
  },
  "@storybook/aem-cli": {
    "storybookLocation": ".storybook",
    "localPackagePath": ".storybook/storybook-library/",
    "uiApps": "./components",
    "componentPaths": [
      "./components"
    ],
    "storybookAEMStyleSystem": false,
    "storybookAEMGrid": false,
    "storybookAEMPageTemplate": false,
    "storybookAEMConfluence": false,
    "storybookAEMFoundation": false,
    "aemContentDefaultPageResourceType": "storybook/components/structure/page",
    "aemContentDefaultPageTemplate": "/conf/storybook/settings/wcm/templates/content-page",
    "aemContentDefaultPageContentPath": "/root/regioncontainer-main/responsivegrid",
    "aemStoryHeadingComponentResourceType": "storybook/components/content/text",
    "aemStoryHeadingComponentTitleProperty": "jcr:title",
    "storyRoot": "Components",
    "packageGroup": "storybook",
    "packageName": "storybook-library.zip",
    "aemContentPath": "/content/storybook/storybook-library"
  }
}
